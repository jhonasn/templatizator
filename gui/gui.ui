<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="window">
    <property name="height">200</property>
    <property name="padding">5</property>
    <property name="takefocus">true</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="columnspan">1</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
      <property name="rowspan">1</property>
      <columns>
        <column id="0">
          <property name="pad">0</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="ttk.Labelframe" id="project_labelframe">
        <property name="height">50</property>
        <property name="text" translatable="yes">Projeto:</property>
        <property name="width">300</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="project_file_label">
            <property name="anchor">w</property>
            <property name="cursor">arrow</property>
            <property name="relief">sunken</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">Selecione um arquivo...</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="project_file_button">
            <property name="text" translatable="yes">Selecione</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="configuration_labelframe">
        <property name="height">50</property>
        <property name="text" translatable="yes">Salvar templates em:</property>
        <property name="width">300</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="ttk.Label" id="configuration_file_label">
            <property name="anchor">w</property>
            <property name="cursor">arrow</property>
            <property name="relief">sunken</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">Selecione um arquivo...</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="configuration_file_button">
            <property name="text" translatable="yes">Selecione</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="variables_labelframe">
        <property name="height">100</property>
        <property name="text" translatable="yes">Variáveis de substituição:</property>
        <property name="width">300</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property name="sticky">nw</property>
          <rows>
            <row id="3">
              <property name="weight">0</property>
            </row>
          </rows>
        </layout>
        <child>
          <object class="ttk.Label" id="Label_3">
            <property name="text" translatable="yes">Nome:</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="variable_nome_entry">
            <property name="justify">left</property>
            <layout>
              <property name="column">0</property>
              <property name="padx">2</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="Label_4">
            <property name="text" translatable="yes">Valor:</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="variable_value_entry">
            <property name="justify">left</property>
            <property name="text" translatable="yes">Entry_2</property>
            <layout>
              <property name="column">1</property>
              <property name="padx">2</property>
              <property name="propagate">False</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="variable_action_button">
            <property name="text" translatable="yes">+</property>
            <property name="width">0</property>
            <layout>
              <property name="column">3</property>
              <property name="propagate">False</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="pygubu.builder.widgets.scrolledframe" id="variables_scrollframe">
            <property name="scrolltype">vertical</property>
            <property name="usemousewheel">false</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">3</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
            <child>
              <object class="ttk.Treeview" id="variables_treeview">
                <property name="selectmode">extended</property>
                <property name="show">headings</property>
                <bind add="True" handler="callback" sequence="&lt;1&gt;" />
                <layout>
                  <property name="column">0</property>
                  <property name="columnspan">3</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
                <child>
                  <object class="ttk.Treeview.Column" id="variable_name_treeviewcolumn">
                    <property name="column_anchor">w</property>
                    <property name="heading_anchor">w</property>
                    <property name="minwidth">20</property>
                    <property name="stretch">true</property>
                    <property name="tree_column">false</property>
                    <property name="visible">true</property>
                    <property name="width">200</property>
                  </object>
                </child>
                <child>
                  <object class="ttk.Treeview.Column" id="variable_value_treeviewcolumn">
                    <property name="column_anchor">w</property>
                    <property name="minwidth">20</property>
                    <property name="stretch">true</property>
                    <property name="tree_column">false</property>
                    <property name="visible">true</property>
                    <property name="width">200</property>
                  </object>
                </child>
                <child>
                  <object class="ttk.Treeview.Column" id="variable_action_treeviewcolumn">
                    <property name="column_anchor">w</property>
                    <property name="heading_anchor">w</property>
                    <property name="minwidth">20</property>
                    <property name="stretch">true</property>
                    <property name="tree_column">false</property>
                    <property name="visible">true</property>
                    <property name="width">200</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="project_selected_labelframe">
        <property name="height">300</property>
        <property name="text" translatable="yes">Projeto selecionado:</property>
        <property name="width">300</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
          <property name="sticky">nw</property>
        </layout>
        <child>
          <object class="pygubu.builder.widgets.scrolledframe" id="project_scrolledframe">
            <property name="scrolltype">both</property>
            <property name="usemousewheel">false</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="ttk.Treeview" id="project_treeview">
                <property name="selectmode">extended</property>
                <property name="show">tree</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">False</property>
                  <property name="row">0</property>
                </layout>
                <child>
                  <object class="ttk.Treeview.Column" id="project_files_treeviewcolumn">
                    <property name="column_anchor">w</property>
                    <property name="heading_anchor">w</property>
                    <property name="minwidth">20</property>
                    <property name="stretch">true</property>
                    <property name="tree_column">false</property>
                    <property name="visible">true</property>
                    <property name="width">200</property>
                  </object>
                </child>
                <child>
                  <object class="ttk.Treeview.Column" id="project_actions_treeviewcolumn">
                    <property name="column_anchor">w</property>
                    <property name="heading_anchor">w</property>
                    <property name="minwidth">20</property>
                    <property name="stretch">true</property>
                    <property name="tree_column">false</property>
                    <property name="visible">true</property>
                    <property name="width">200</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Toplevel" id="editor_toplevel">
    <property name="height">200</property>
    <property name="width">200</property>
  </object>
</interface>
